{
  "name": "dynamodb-as-cache",
  "version": "0.1.0",
  "description": "Use dynamodb as cache service, providing Redis-like APIs",
  "main": "index.js",
  "keywords": [
    "DynamoDB",
    "Cache",
    "Redis"
  ],
  "scripts": {
    "test": "./node_modules/jest/bin/jest.js --coverage --collectCoverageFrom=src/**/*.js --detectOpenHandles --forceExit",
    "example1": "node -r dotenv/config examples/set_and_get.js"
  },
  "author": "Matt Shen",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.900.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^26.6.3"
  },
  "jest": {
    "testRegex": "./__tests__/.*\\.(test|spec)\\.js$",
    "testEnvironment": "node",
    "globals": {
      "request": {}
    },
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "lib"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "src/lib/**/*.js",
      "!src/index.js",
      "!src/lib/repository/*.js"
    ]
  }
}
